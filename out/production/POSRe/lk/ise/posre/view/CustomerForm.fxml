            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnAddNewCustomer" buttonType="RAISED" layoutX="572.0" layoutY="27.0" onAction="#addNewCustomerOnAction" prefHeight="30.0" prefWidth="156.0" text="Add new Customer" textFill="#391dcf">
