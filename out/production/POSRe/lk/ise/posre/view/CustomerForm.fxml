         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtName" labelFloat="true" layoutX="173.0" layoutY="110.0" prefHeight="30.0" prefWidth="156.0" promptText="name">
