<AnchorPane fx:id="customerFormContext" prefHeight="478.0" prefWidth="742.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ise.posre.control.CustomerFormController">
